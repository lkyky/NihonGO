    <string name="menu_b_lesson1_pg2">Beginner Lesson 1 Pg2</string>
    <string name="txtBLesson1Pg2"><b>Two [2]</b>\n 二（に）ni\n</string>
    <string name="txtBLesson1Pg2Ex"><b>Example:</b>\n 二時（にじ）niji\n = Two o\' Clock \n</string>
    <string name="imgNumTwo">Number Two</string>
    <string name="imgTwoOClock">Two o\' Clock</string>
    <string name="menu_b_lesson1_pg3">Beginner Lesson 1 Pg3</string>
    <string name="txtBLesson1Pg3"><b>Three [3]</b>\n 三（さん）san\n</string>
    <string name="txtBLesson1Pg3Ex"><b>Example:</b>\n 三人（さんにん）sannin\n = Three Person\n</string>
    <string name="imgNumThree">Number Three</string>
    <string name="imgThreePerson">Three Person</string>
    <string name="menu_b_lesson1_pg4">Beginner Lesson 1 Pg4</string>
    <string name="txtBLesson1Pg4"><b>Four [4]</b>\n 四（し／よん）shi/ yon\n</string>
    <string name="txtBLesson1Pg4Ex"><b>Example:</b>\n 四月（しがつ）shigatsu\n = April\n</string>
    <string name="imgNumFour">Number Four</string>
    <string name="imgApril">April</string>
    <string name="menu_b_lesson1_pg5">Beginner Lesson 1 Pg5</string>
    <string name="txtBLesson1Pg5"><b>Five [5]</b>\n 五（ご）go\n</string>
    <string name="txtBLesson1Pg5Ex"><b>Example:</b>\n 五円（ごえん）goen\n = Five Yen\n</string>
    <string name="imgNumFive">Number Five</string>
    <string name="imgFiveYen">Five Yen</string>
