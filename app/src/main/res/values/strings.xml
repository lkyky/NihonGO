    <string name="txtBLesson1Pg1"><b>One [1]</b>\n 一（いち）ichi\n</string>
    <string name="txtBLesson1Pg1Ex"><b>Example:</b>\n 一年生（いちねんせい）ichinensei\n = First Year Student\n</string>
